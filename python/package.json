{
    "name": "webviz-subsurface-components",
    "version": "1.0.0",
    "description": "Custom Dash components for use in Webviz. Note: This package is not intended to be published on npm.",
    "repository": {
        "type": "git",
        "url": "git://github.com/equinor/webviz-subsurface-components.git"
    },
    "bugs": {
        "url": "https://github.com/equinor/webviz-subsurface-components/issues"
    },
    "homepage": "https://github.com/equinor/webviz-subsurface-components",
    "author": "Equinor <opensource@equinor.com>",
    "license": "MPL",
    "main": "dist/index.js",
    "scripts": {
        "preinstall": "cd ../typescript && npm ci && npx nx run-many -t build",
        "build:js": "webpack",
        "tidy-up": "rimraf ./dash-components",
        "copy-components": "copyfiles -e \"./src/components/*/*.test.tsx\" -e \"./src/components/*/*.stories.tsx\" \"./src/components/*/*.{tsx,jsx}\" ./dash-components/ && copyfiles \"./src/components/*/**/*.{ts,js}\" ./dash-components/",
        "generate-dash-components": "dash-generate-components ./dash-components webviz_subsurface_components -p package-info.json --ignore '(.test.|VectorSelector)'",
        "build:py": "npm run tidy-up && npm run copy-components && npm run generate-dash-components && npm run tidy-up",
        "build": "npm run build:js && npm run build:py",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "typecheck": "tsc --noEmit",
        "lint": "eslint *.js *.json \"src/**/*.{js,jsx,ts,tsx,mdx,json}\"",
        "format": "npm run lint -- --fix",
        "validate": "npm run typecheck && npm run lint"
    },
    "dependencies": {
        "@deck.gl/core": "^8.8.25",
        "@emerson-eps/color-tables": "^0.4.61",
        "@equinor/eds-core-react": "^0.32.3",
        "@equinor/eds-icons": "^0.19.1",
        "@mui/material": "^5.11",
        "@reduxjs/toolkit": "^1.7.2",
        "@webviz/dash-components": "file:../typescript/packages/dash-components/dist",
        "@webviz/group-tree": "file:../typescript/packages/group-tree/dist",
        "@webviz/subsurface-viewer": "file:../typescript/packages/subsurface-viewer/dist",
        "@webviz/well-completions-plot": "file:../typescript/packages/well-completions-plot/dist",
        "@webviz/well-log-viewer": "file:../typescript/packages/well-log-viewer/dist",
        "@webviz/wsc-common": "file:../typescript/packages/wsc-common/dist",
        "convert-units": "^2.3.4",
        "mathjs": "^9.4.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-dropdown-tree-select": "^2.8.0",
        "react-redux": "^8.1.1",
        "react-resize-detector": "^9.0.0"
    },
    "devDependencies": {
        "@actions/core": "^1.9.1",
        "@plotly/webpack-dash-dynamic-import": "^1.3.0",
        "@storybook/addon-essentials": "^7.0.23",
        "@storybook/addon-links": "^7.0.23",
        "@storybook/addon-mdx-gfm": "^7.0.23",
        "@storybook/addon-storysource": "^7.0.23",
        "@storybook/react": "^7.0.23",
        "@storybook/react-webpack5": "^7.0.23",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^14.0.0",
        "@testing-library/user-event": "^13.1.9",
        "@types/convert-units": "^2.3.5",
        "@types/jest": "^29.5.2",
        "@types/react": "^18.2.7",
        "@types/react-dom": "^18.2.7",
        "@typescript-eslint/eslint-plugin": "^6.4.0",
        "@typescript-eslint/parser": "^6.4.0",
        "copyfiles": "^2.0.0",
        "css-loader": "^6.7.1",
        "eslint": "^8.47.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-import": "^2.28.0",
        "eslint-plugin-mdx": "^2.2.0",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "file-loader": "^6.2.0",
        "jest": "^29.6.0",
        "react-docgen": "^5.4.0",
        "redux-mock-store": "^1.5.4",
        "sass": "^1.66.1",
        "sass-loader": "^13.3.2",
        "storybook": "^7.0.23",
        "style-loader": "^3.3.1",
        "ts-loader": "^9.3.1",
        "typescript": "^4.7.4",
        "url-loader": "^4.1.1",
        "webpack": "^5.73.0",
        "webpack-cli": "^4.10.0"
    }
}
